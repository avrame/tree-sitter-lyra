{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "lyra_parser",
      "camelcase": "LyraParser",
      "title": "The Lyra Programming Language Parser",
      "scope": "source.lyra_parser",
      "file-types": [
        ".lyra"
      ],
      "injection-regex": "^lyra_parser$",
      "class-name": "TreeSitterLyraParser"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree-sitter parser for the Lyra Programming Language",
    "authors": [
      {
        "name": "Avram Eisner",
        "email": "avrame@gmail.com",
        "url": "https://www.avrameisner.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-lyra_parser"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}